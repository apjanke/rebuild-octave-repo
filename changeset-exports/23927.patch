+void
+main_window::force_refresh_workspace()
+{
+  octave::symbol_table::scope *scope
+   = octave::__get_current_scope__ ("main_window::load_workspace_callback");
+
+  if (scope)
+    octave_link::set_workspace (true, scope->workspace_info (), false);
+}
