+
+      Qt::KeyboardModifiers modifiers = e->modifiers ();
+
+      if (modifiers & Qt::ShiftModifier)
+        key += Qt::SHIFT;
+      if (modifiers & Qt::ControlModifier)
+        key += Qt::CTRL;
+      if (modifiers & Qt::AltModifier)
+        key += Qt::ALT;
+      if (modifiers & Qt::MetaModifier)
+        key += Qt::META;
+
+      setText (QKeySequence (key).toString ());
+    }
+}
