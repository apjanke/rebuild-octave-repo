+  idx = (x == 0) & (a == 0);
+  if (any (idx))
+    y(idx) = gammainc_00 (tail);
+    todo(idx) = false;
+  endif
+
+  ## Case 2: x == 0.
+  idx = todo & (x == 0);
+  if (any (idx))
+    y(idx) = gammainc_x0 (tail);
+    todo(idx) = false;
+  endif
+
+  ## Case 3: x = Inf
+  idx = todo & (x == Inf);
+  if (any (idx))
+    y(idx) = gammainc_x_inf (tail);
+    todo(idx) = false;
+  endif
+
+  ## Case 4: a = Inf
+  idx = todo & (a == Inf);
+  if (any (idx))
+    y(idx) = gammainc_a_inf (tail);
+    todo(idx) = false;
+  endif
+
+  ## Case 5: a == 0.
+  idx = todo & (a == 0);
+  if (any (idx))
+    y(idx) = gammainc_a0 (x(idx), tail);
+    todo(idx) = false;
+  endif
+
+  ## Case 6: a == 1.
+  idx = todo & (a == 1);
+  if (any (idx))
+    y(idx) = gammainc_a1 (x(idx), tail);
+    todo(idx) = false;
+  endif
+
+  ## Case 7: positive integer a; exp (x) and a! both under 1/eps.
+  idx = (todo
+         & (a == fix (a)) & (a > 1) & (a <= 18) & (x <= 36) & (abs (x) >= .1));
