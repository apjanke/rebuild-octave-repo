+file_editor_tab::new_file ()
+{
+  if (!_file_editor->isVisible ())
+    {
+      _file_editor->show ();
+    }
+
+  set_file_name (UNNAMED_FILE);
+  update_window_title (false); // window title (no modification)
+  _edit_area->setText ("");
+  _edit_area->setModified (false); // new file is not modified yet
+}
