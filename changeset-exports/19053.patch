+%!error [L,U] = iluc(A_tiny, 1, 'foo');
+%!error [L,U] = iluc(A_tiny, 1, '');
+%!error [L,U] = iluc(A_tiny, 1, 1);
+%!error [L,U] = iluc(A_tiny, 1, [1,2]);
