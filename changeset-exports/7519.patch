-      endif
-
-      if (a16 != b16)
-	error(["failed: " file " uint16"]);
-      endif
-
-      if (a17 != b17)
-	error(["failed: " file " uint32"]);
-      endif
-
-      if (a18 != b18)
-	error(["failed: " file " uint64"]);
-      endif
-
-      if (a19 != b19)
-	error(["failed: " file " sparse"]);
-      endif
