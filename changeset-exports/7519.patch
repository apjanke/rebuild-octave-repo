-
-t(++i) = assert(r, unwrap(w), tol);               #unwrap single row
-t(++i) = assert(r', unwrap(w'), tol);             #unwrap single column
-t(++i) = assert([r',r'], unwrap([w',w']), tol);   #unwrap 2 columns
-t(++i) = assert([r;r], unwrap([w;w],[],2), tol);  #verify that dim works
