                                                  QDir::Files | QDir::Readable,
                                                  QDir::Name);
   for (int i = 0; i < qm_files.length (); i++)   // insert available languages
@@ -617,16 +619,18 @@ settings_dialog::write_changed_settings 
   delete lexer;
