-  char opt;
-  if (milu == "row")
-    opt = ROW;
-  else if (milu == "col")
-    opt = COL;
-  else
-    opt = OFF;
-
-  octave_idx_type jrow, i, j, k, jj, total_len_l, total_len_u, max_len_u,
